{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      ["module-resolver", {
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
        "@api":"./src/api",
         "@pages": "./src/pages",
          "@components": "./src/components"        
        }
      }]
    ]
  }